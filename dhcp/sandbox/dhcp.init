create host name eth0
set int state host-eth0 up
set int ip address host-eth0 9.52.1.13/24
set interface mac address host-eth0 0025.2e2d.75fa 

packet-generator new {
  name s2
  limit 1
  node ethernet-input
  size 539-539
  no-recycle
  data {
    0x0800: ffff.ffff.ffff -> ffff.ffff.ffff
    hex 
  }
}

trace add pg-input 100
trace add ethernet-input 100

set int state pg0 up

disc addrswap pg0
disc addrswap host-eth0


